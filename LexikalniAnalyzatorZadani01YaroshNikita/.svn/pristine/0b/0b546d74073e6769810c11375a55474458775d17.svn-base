/**
 *
 * @author
 */
public abstract class Token {

    //TODO Doplnit atribut, tÄ›lo konstruktoru a metod
    
    private final TokenType type;

    public Token(TokenType type) {
        this.type = type;
    }

    public TokenType getType() {
